@model WebbyMyself.Models.Branch

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="~/css/site.css"> 
<link rel="script" href="~/js/site.js">
<script src="~/lib/font-awesome/js/all.js"></script>
<link href="~/lib/font-awesome/css/all.css"/>

<h1 class="title">Thông tin ngành</h1>
<br />
<p>
    <div>
        <dl class="row">
            <dt class = "col-sm-4">
                    Tên ngành
            </dt>
            <dd class = "col-sm-8">
                    @Html.DisplayFor(model => model.BranchName)
            </dd>
        </dl>
        <div>
            <a class="btn btn-secondary" asp-action="Index">Trở về trang chủ</a> 
        </div>
    </div>
</p>
